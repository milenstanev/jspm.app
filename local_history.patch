-        docs: {
-            description: "enforce dot notation whenever possible",
-            category: "Best Practices",
-            recommended: false
-        },
-
-        schema: [
-            {
-                type: "object",
-                properties: {
-                    allowKeywords: {
-                        type: "boolean"
